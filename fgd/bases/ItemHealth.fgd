	HealthMultiplier[MBase](float) : "Health Multiplier" : "1.0" : "Multiplies the amount of health this item gives."
	
	// Inputs
	input SetHealthMultiplier[MBase](float) : "Sets the multiplier for the amount of health this item gives."
	]
